class Solution:
    def findMedianSortedArrays(self, nums1: List[int], nums2: List[int]) -> float:
        nums3 = nums1+nums2
        nums3.sort()
        nl = len(nums3)
        if nl%2 == 0:
            small = (nums3[int(nl/2)-1])
            big = nums3[int(nl/2)]
            return (small+big)/2
        else:
            return nums3[floor(nl/2)]
